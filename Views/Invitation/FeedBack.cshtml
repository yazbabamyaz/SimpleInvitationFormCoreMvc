@model Invitation

<div>
    <h3 class="display-4">Thank you @Model.FullName </h3>
    <p class="lead">  Your invitation has been  <mark>accepted.</mark></p>
    <p class="lead">We will be happy to see you with us.</p>
    <hr />
   
    <p class="text-muted">   Your application was received in @Model.ApplyDate.</p>

    <div class="btn-group">
        <a class="btn btn-outline-success" asp-action="Index">Home</a>
        
    </div>
</div>
